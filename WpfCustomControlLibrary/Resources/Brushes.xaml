<ResourceDictionary x:Class="WpfCustomControlLibrary.Resources.Brushes"
                    x:ClassModifier="internal"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:opt="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
                    xmlns:res="clr-namespace:WpfCustomControlLibrary.Resources">

    <SolidColorBrush opt:Freeze="True" x:Key="{x:Static res:Keys.ControlBackgroundBrushKey}" Color="Blue" />

</ResourceDictionary>
